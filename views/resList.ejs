<div class="d-flex flex-wrap text-center overflow-scroll">
    <% for (restaurant of restaurants){%>
        <div class="p-3 m-3 border border-primary bg-light w-33">
            <a href="/resList/remove/<%= restaurant._id %>">Remove from List</a>
            <br>
            Restaurant Name: <%= restaurant.name %>
            <br>
            <%= restaurant.cuisine %>
            <br>
            <%= restaurant.borough %>
            <br>
            
        </div>
        
    <% } %>
</div>

